<?xml version="1.0" encoding="UTF-8"?>
<suppressions xmlns="https://jeremylong.github.io/DependencyCheck/dependency-suppression.1.3.xsd">
    <!--
    This file contains suppressions for OWASP Dependency Check false positives.
    Each suppression should include:
    1. A clear reason for suppression
    2. An expiration date for review
    3. Specific CVE or vulnerability identifier

    Example suppression:
    <suppress>
        <notes><![CDATA[
        This vulnerability affects a development dependency only and is not present in production builds.
        Scheduled for review: 2025-12-01
        ]]></notes>
        <packageUrl regex="true">^pkg:npm/example-dev-package@.*$</packageUrl>
        <cve>CVE-2023-12345</cve>
    </suppress>
    -->

    <!-- Add specific suppressions here as needed -->
    <!--
    Example: Suppress development-only dependencies
    <suppress>
        <notes><![CDATA[
        Karma is a development dependency used only for testing.
        Not included in production builds.
        Review date: 2025-12-01
        ]]></notes>
        <packageUrl regex="true">^pkg:npm/karma@.*$</packageUrl>
    </suppress>
    -->
</suppressions>
